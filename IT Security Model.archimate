  <folder name="Application" id="id-a700e8dc51fc43f4a0a86e58e448ba67" type="application">
    <element xsi:type="archimate:ApplicationService" name="Application Service" id="id-f7707dd6ce6d43f99ca2b8c8ca4d2fca"/>
    <element xsi:type="archimate:ApplicationService" name="Management Controls" id="id-4520bbbb4d0843ff9fa4a64b7fe0807f"/>
    <element xsi:type="archimate:DataObject" name="IT Asset" id="id-29f85df67f4b4ae3aa0a6f13143c8927">
      <documentation>GC Definition:  The components of an information system, including business applications, data, hardware, and software (ITSG-33/HTRA)</documentation>
    </element>
    <element xsi:type="archimate:ApplicationService" name="Existing Security Controls" id="id-eb4ebe1262fc4d67b0b9ffd6d015737e"/>
